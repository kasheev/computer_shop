@model BasaDate.Models.session

@{
    ViewBag.Title = "Подробнее";
}

<h2>Подробнее</h2>

<div>
    <h4>Сеансы</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <p>Дата</p>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.date)
        </dd>

        <dt>
            <p>Начало</p>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.start_time)
        </dd>

        <dt>
            <p>Конец</p>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.end_time)
        </dd>

        <dt>
            <p>Администратор</p>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.admin.full_name)
        </dd>

        <dt>
            <p>Номер компьютера</p>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.id_personal_computer)
        </dd>

        <dt>
            <p>Услуга</p>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.service.title)
        </dd>

        <dt>
            <p>Посетитель</p>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.visitor.full_name)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Изменить", "Edit", new { id = Model.id }) |
    @Html.ActionLink("Назад", "Index")
</p>
